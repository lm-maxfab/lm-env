{
  "name": "lm-env",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "init": "git submodule init && git submodule update",
    "postinstall": "cd lm-app && npm i && cd ../lm-statics && npm i",
    "env:build:dev": "node ./scripts/env.build.dev.js",
    "env:serve:dev": "NODE_ENV=developpment PORT=3000 node --trace-warnings --experimental-specifier-resolution=node ./dist/dev/env/index.js",
    "env:start:dev": "npx concurrently -n 'env:build:dev,env:serve:dev' 'npm run env:build:dev' 'npm run env:serve:dev'",
    "env:build:watch:dev": "node ./scripts/env.build.watch.dev.js",
    "env:serve:watch:dev": "NODE_ENV=developpment PORT=3000 npx -y nodemon --watch dist/dev/env --trace-warnings --experimental-specifier-resolution=node ./dist/dev/env/index.js",
    "env:start:watch:dev": "npx concurrently -n 'env:build:watch:dev,env:serve:watch:dev' 'npm run env:build:watch:dev' 'npm run env:serve:watch:dev'",
    "statics:build:dev": "node ./scripts/statics.build.dev.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.22",
    "chalk": "^5.0.1",
    "clean-css": "^5.3.0",
    "dir-compare": "^4.0.0",
    "fs-extra": "^10.0.1",
    "highlight.js": "^11.5.0",
    "jsdom": "^19.0.0",
    "marked": "^4.0.12",
    "nodemon": "^2.0.15",
    "sass": "^1.49.11",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2"
  }
}
